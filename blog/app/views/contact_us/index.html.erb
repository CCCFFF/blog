<div class="container">
  <h1>Contact Us</h1>
  <%= form_tag(@inquiry), method: 'post' do %>

  <div>
    <%= label_tag :name %><br>
    <%= text_field_tag :name %>
  </div>

  <div>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>

  <div>
    <%= label_tag :message %>
    <%= text_area_tag :message %>
  </div>

  <div>
    <%= submit_tag "Send" %>
  </div>
<% end %>
</div>
